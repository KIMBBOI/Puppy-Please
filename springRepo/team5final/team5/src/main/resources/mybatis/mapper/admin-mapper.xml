<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="AdminMapper">
  <select id="login" resultType="AdminVo">
  	SELECT 
  		ADMIN_NO
  	FROM ADMIN
  	WHERE ID=#{id}
  	AND PWD = #{pwd}
  </select>
  
</mapper>